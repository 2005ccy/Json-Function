{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": false,
    "experimentalDecorators": true,
    "lib": ["dom", "es6", "es5", "es2017"],
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "interface/*": ["./src/interface/*"],
      "utils/*": ["./src/utils/*"]
    }
  },
  "include": ["./src/package/**/*"],
  "exclude": ["./src/interface/*"],
  "compileOnSave": false
}
